---
title: Work
description: Ajaibghar Projects Page
layout: layouts/base.njk
---
<div class="" x-init="console.log('Hello from Alpine.js')">
    <h1>{{ title }}</h1>
    {% set projlist = collections.projects %}

    <div>
    {% for post in projlist %}
            <a class="" href="{{ post.url | url }}">{{ post.data.title }}
            <img src="{{ post.data.featimg }}" width="180px">
            {% for tag in post.data.chips %} 
            <p>{{tag}}</p>
            {% endfor%}
            </a>
    {% endfor %}
    </div>
</div>