---
description: Individual Project Page
layout: layouts/base.njk
permalink: "work/{{ title }}/index.html"

---
<div class="flex flex-col font-work snap-proximity snap-y text-white">
    <div class="-z-10 flex flex-col justify-center header  bg-cover bg-center bg-blend-darken snap-center mb-8"  >
        {# <img src="{{featimg}}" class="-z-20"> #}
        <div class="px-5 mx-auto text-center">
            <h1 class="text-white text-6xl font-bold">{{title}}</h1>
            {# <p class="text-white text-2xl font-regular">{{projprov}}</p> #}
        </div>
    </div>

    <div class="main flex flex-col flex-wrap justify-evenly items-center snap-center">

        {# px-16 my-16 #}

        <div class="projectcontent content-center md:max-w-prose px-6">
            {% block content %}
                <p class="text-2xl">
                    {{ content | safe }}</p>
            {% endblock %}
        </div>
        <div class="vid  {{ 'hidden' if 'none' in project_videos }} snap-center">
            <iframe class="w-screen md:h-full md:w-[42rem] aspect-video" src="{{project_videos}}"></iframe>
        </div>
        {# {% endif %} #}
        {# <p>SOME IMAGES</p> #}
        <div class=" py-12 flex flex-col flex-wrap justify-evenly items-center snap-center">
            {%- for img in projimg -%}
                <div class="">
                    <img src="{{ img.img }}" alt="{{ img.caption }}" class="object-cover p-2 h-full w-[42rem]">
                    {# <p class="text-sm">{{ img.caption}}</p> #}
                </div>
                {%- endfor-%}
            </div>
            <div class="collaborators px-5 ">
                <p class="text-2xl font-extralight">COLLABORATORS</p>
                <ul class="text-lg font-regular list-none capitalize">
                    {%- for person in collaborators -%}
                        <li>{{ person.collaborators }}</li>
                    {%- endfor -%}
                </ul>
            </div>
        </div>

    </div>