---
description: Individual Project Page
layout: layouts/base.njk
permalink: "work/{{ title }}/index.html"

---
{% set projlist = collections.projects %}
<div class="md:flex md:flex-row ">
        {% include "components/navlist.njk" %}

    <div class="md:w-2/3 text-white px-6">
         <div class="">
            <h1 class="">{{title}}</h1>
             <img src="{{featimg}}" alt="test" class='w-full mb-8'>

        <div class="projectcontent text-md font-work max-w-prose mb-8">
            {% block content %}
                <p class="text-2xl">
                    {{ content | safe }}</p>
            {% endblock %}
        </div>
 <div class=" py-12 flex flex-col flex-wrap">
            {%- for img in projimg -%}
                <div class="">
                    <img src="{{ img.img }}" alt="{{ img.caption }}" class="object-cover p-2 h-full w-[42rem]">
                </div>
                {%- endfor-%}
            </div>
        </div>
        <div class=" py-12 flex flex-col flex-wrap mb-10">
            <h2 class="uppercase text-lg">Collaborators</h2>
            {%- for collaborator in collaborators -%}
                <div class="text-md">
                    <ul>
                    <li class="">{{ collaborator.collaborators }}</li>
                    </ul>
                </div>
                {%- endfor-%}
            </div>
        </div>        </div>
