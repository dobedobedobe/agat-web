---
description: Individual Project Page
layout: layouts/base.njk
permalink: "work/{{ title }}/index.html"

---
<div class="font-work text-white md:mx-5 m-2 px-1 space-y-3 my-auto">
    <div class="-z-10"  >
        {# <img src="{{featimg}}" class="-z-20"> #}
        <div class="">
            <h1 class="">{{title}}</h1>
            {# <p class="text-white text-2xl font-regular">{{projprov}}</p> #}
        </div>
    </div>

    <div class="main">

        {# px-16 my-16 #}

        <div class="projectcontent text-xs font-work max-w-prose mb-8">
            {% block content %}
                <p class="text-2xl">
                    {{ content | safe }}</p>
            {% endblock %}
        </div>
        <div class="vid  {{ 'hidden' if 'none' in project_videos }} snap-center">
            <iframe class="w-screen md:h-full md:w-[42rem] aspect-video" src="{{project_videos}}"></iframe>
        </div>
        {# {% endif %} #}
        {# <p>SOME IMAGES</p> #}
        <div class=" py-12 flex flex-col flex-wrap">
            {%- for img in projimg -%}
                <div class="">
                    <img src="{{ img.img }}" alt="{{ img.caption }}" class="object-cover p-2 h-full w-[42rem]">
                    {# <p class="text-sm">{{ img.caption}}</p> #}
                </div>
                {%- endfor-%}
            </div>
            <div class="collaborators mb-20">
                <p class="text-sm font-extralight">COLLABORATORS</p>
                <ul class="text-xs font-regular list-none capitalize">
                    {%- for person in collaborators -%}
                        <li>{{ person.collaborators }}</li>
                    {%- endfor -%}
                </ul>
            </div>
        </div>

    </div>